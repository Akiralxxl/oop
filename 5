#include <iostream>
#include <vector>
#include <string>

template <typename T>
class Queue {
private:
    std::vector<T> elements;

public:
    void push(const T& item) {
        elements.push_back(item);
        std::cout << "Added: " << item << std::endl;
    }

    T pop() {
        if (elements.empty()) {
            throw std::out_of_range("Queue is empty!");
        }
        T first = elements.front();
        elements.erase(elements.begin());
        std::cout << "Removed: " << first << std::endl;
        return first;
    }

    void display() const {
        if (elements.empty()) {
            std::cout << "Queue is empty" << std::endl;
            return;
        }
        std::cout << "Queue contents: ";
        for (const auto& item : elements) {
            std::cout << item << " ";
        }
        std::cout << std::endl;
    }
};

int main() {
    std::cout << "=== Integer Queue ===" << std::endl;
    Queue<int> intQueue;

    intQueue.push(100);
    intQueue.push(200);
    intQueue.push(300);
    intQueue.display();

    intQueue.pop();
    intQueue.display();

    intQueue.push(400);
    intQueue.display();

    std::cout << "\n=== String Queue ===" << std::endl;
    Queue<std::string> stringQueue;

    stringQueue.push("Alpha");
    stringQueue.push("Beta");
    stringQueue.push("Gamma");
    stringQueue.display();

    stringQueue.pop();
    stringQueue.display();

    stringQueue.push("Delta");
    stringQueue.display();

    return 0;
}
